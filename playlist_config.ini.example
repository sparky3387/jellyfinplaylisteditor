[Paths]
# Base path to your music library
# For windows file systems an example is C:\\Music\\Directory
newpath_location = /mnt/media/music/
# Path to ffprobe executable for audio metadata
ffprobe_path = /usr/lib/jellyfin-ffmpeg/ffprobe
# Directory containing your M3U8 playlists
playlist_directory = ./playlists

[Settings]
# Logging level: DEBUG, INFO, WARNING, ERROR
log_level = INFO

[Jellyfin]
# Jellyfin server URL (e.g., http://localhost:8096 or https://jellyfin.example.com)
server_url = http://localhost:8096
# API key (create one in the Jellyfin dashboard under Admin > API Keys)
api_key = XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

